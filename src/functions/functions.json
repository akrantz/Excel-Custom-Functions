{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/office-js/custom-functions.schema.json",
  "functions": [
    {
      "id": "CONTOSO.ADD",
      "name": "CONTOSO.ADD",
      "description": "Add two numbers",
      "helpUrl": "http://www.contoso.com/help",
      "result": {
        "type": "number",
        "dimensionality": "scalar"
      },
      "parameters": [
        {
          "name": "first",
          "description": "first number to add",
          "type": "number",
          "dimensionality": "scalar"
        },
        {
          "name": "second",
          "description": "second number to add",
          "type": "number",
          "dimensionality": "scalar"
        }
      ]
    },
    {
      "id": "CONTOSO.INCREMENT",
      "name": "CONTOSO.INCREMENT",
      "description": "Periodically increment a value",
      "helpUrl": "http://www.contoso.com/help",
      "result": {
            "type": "number",
            "dimensionality": "scalar"
      },
      "parameters": [
        {
          "name": "increment",
          "description": "Amount to increment",
          "type": "number",
          "dimensionality": "scalar"
        }
      ],
      "options": {
        "cancelable": true,
        "stream": true
      }
    },
    {
      "id": "CONTOSO.CLOCK",
      "name": "CONTOSO.CLOCK",
      "description": "Show the current time.",
      "helpUrl": "http://www.contoso.com/help",
      "result": {
        "type": "string",
        "dimensionality": "scalar"
      },
      "parameters": [
      ],
      "options": {
        "cancelable": true,
        "stream": true
      }
    },
    {
      "id": "CONTOSO.LOG",
      "name": "CONTOSO.LOG",
      "description": "Write a message to the log.",
      "helpUrl": "http://www.contoso.com/help",
      "result": {
        "type": "string",
        "dimensionality": "scalar"
      },
      "parameters": [
        {
          "name": "message",
          "description": "The message.",
          "type": "string",
          "dimensionality": "scalar"
        }
      ]
    },
    {
      "id": "CONTOSO.JAVASCRIPT",
      "name": "CONTOSO.JAVASCRIPT",
      "description": "Evaluate JavaScript",
      "helpUrl": "http://www.contoso.com/help",
      "result": {
        "type": "any",
        "dimensionality": "scalar"
      },
      "parameters": [
        {
          "name": "script",
          "description": "JavaScript to execute.",
          "type": "string"
        }
      ]
    },
    {
      "id": "CONTOSO.REGEX.TEST",
      "name": "CONTOSO.REGEX.TEST",
      "description": "Test whether a string matches a regular expression.",
      "helpUrl": "http://www.contoso.com/help",
      "result": {
        "type": "boolean",
        "dimensionality": "scalar"
      },
      "parameters": [
        {
          "name": "string",
          "description": "String to test.",
          "type": "string"
        },
        {
          "name": "pattern",
          "description": "Regular expression pattern",
          "type": "string"
        }
      ]
    }
  ]
}
