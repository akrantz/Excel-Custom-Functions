  "version": "0.2.0",
  "configurations": [
    {
      "name": "Excel Desktop (Proxy)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/office-addin-debugging/lib/debugging.js",
      "args": ["start", "manifest.xml", "--source-bundle-url-path", "index.win32", "--dev-server", "npm run dev-server", "--packager", "npm run packager"],
      "env": {
        "Path": "${workspaceFolder}/node_modules/.bin;${env:Path};"
      },
      "autoAttachChildProcesses": true,

      //"preLaunchTask": "Debugging: Desktop (Proxy)",
      "postDebugTask": "Stop Debug"
    },
    {
      "name": "Excel Desktop (Direct)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "protocol": "inspector",
      "timeout": 600000,
      "trace": true,
      "preLaunchTask": "Debug: Desktop (Direct)",
      "postDebugTask": "Stop Debug",
      "outFiles": [ "${workspaceFolder}/dist/**/*.js" ]
    },
    // {
